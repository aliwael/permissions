{
  "name": "aliwael/permissions",
  "description": "Laravel package for user permissions management.",
  "autoload": {
    "psr-4": {
      "Aliwael\\Permissions\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Ali Wale",
      "email": "dev.aliwaell@gmail.com",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "type": "library",
  "keywords": [
    "laravel",
    "permissions"
  ],
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.4",
    "orchestra/testbed": "^6.0"
  },
  "require": {
    "php": ">=7.2",
    "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "aliwael\\permissions\\Providers\\PermissionsServiceProvider"
      ]
    }
  }
}
